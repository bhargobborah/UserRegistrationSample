@model IEnumerable<ApplicationUser>

@{
    ViewData["Title"] = "Home Page";
}


<table class="table">
    <tr>
        <th>First Name</th>
        <th>User Name</th>
        <th>Email</th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                <label asp-for="@item.FirstName">@item.FirstName</label>

            </td>
            <td>
                <label asp-for="@item.UserName">@item.UserName</label>

            </td>
            <td>
                <label asp-for="@item.Email">@item.Email</label>

            </td>
        </tr>
    }
</table>